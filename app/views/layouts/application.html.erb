<!DOCTYPE html>
<html>
<head>
  <title>Web</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% cache 'navbar' do %>
		<%= render 'shared/static_page/navbar' %>
	<% end %>
	<div class="container-fluid">
			<% render 'shared/static_page/left_side_navbar' %>

			<%= yield %>

			<% render 'shared/static_page/right_side_navbar' %>
	</div>
</body>
<footer>
	<% render 'shared/static_page/footer' %>
</footer>
</html>
